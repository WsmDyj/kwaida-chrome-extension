    "ant-design-vue": "^3.2.17",
    "axios": "^0.21.1",
    "lodash-es": "^4.17.10",
    "pinia": "^2.0.35",
    "vue": "^3.0.5",
    "vue-router": "^4.0.10",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
